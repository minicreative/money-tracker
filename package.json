{
  "name": "money-tracker",
  "version": "1.0.0",
  "description": "Simple JS app for keeping track of spending",
  "main": "index.js",
  "scripts": {
    "apidocs": "apidoc -i api -o public/docs/",
    "codedocs": "jsdoc -r -c jsdoc.json -d public/code/ -R README.md",
    "app:dev": "webpack --watch --mode development",
    "server:dev": "nodemon --watch api index.js",
    "dev": "npm-run-all -p app:dev server:dev",
    "build": "webpack --mode production",
    "build:full": "npm-run-all -s apidocs codedocs build",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minicreative/money-tracker.git"
  },
  "author": "Tomas Roy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/minicreative/money-tracker/issues"
  },
  "homepage": "https://github.com/minicreative/money-tracker#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "async": "^3.2.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.5",
    "morgan": "^1.9.1",
    "numeral": "^2.0.6",
    "papaparse": "^5.3.0",
    "password-hash": "^1.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-autosuggest": "^10.0.0",
    "react-context-hook": "^1.2.1",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "request": "^2.88.2",
    "underscore": "^1.9.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "apidoc": "^0.17.7",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "jsdoc": "^3.6.5",
    "minimist": ">=1.2.3",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.11"
  }
}
