{
  "name": "money-tracker",
  "version": "1.0.0",
  "description": "Simple JS app for keeping track of spending",
  "main": "index.js",
  "scripts": {
    "apidocs": "apidoc -i api/routes/ -o public/docs/",
    "codedocs": "jsdoc -r api/ -d public/code/",
    "app:dev": "webpack --watch --mode development",
    "server:dev": "nodemon --watch api index.js",
    "dev": "npm-run-all -p app:dev server:dev",
    "build": "webpack --mode production",
    "run": "nodemon index.js",
    "start": "npm-run-all -s build run",
    "start:full": "npm-run-all -s apidocs codedocs build run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minicreative/money-tracker.git"
  },
  "author": "Tomas Roy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/minicreative/money-tracker/issues"
  },
  "homepage": "https://github.com/minicreative/money-tracker#readme",
  "dependencies": {
    "async": "^3.1.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "password-hash": "^1.2.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "apidoc": "^0.17.7",
    "babel-loader": "^8.0.6",
    "jsdoc": "^3.6.3",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
}
